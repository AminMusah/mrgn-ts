{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**", ".next/**"]
    },
    "lint": {
      "outputs": []
    },
    "dev": {
      "cache": false
    },
    "start": {
      "cache": false
    }
  },
  "globalEnv": [
    "LIQUIDATOR_PK",
    "RPC_ENDPOINT",
    "KEYPAIR_PATH",
    "SLEEP_INTERVAL",
    "MRGN_ENV"
  ]
}
